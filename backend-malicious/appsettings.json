{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.AspNetCore.Server.Kestrel": "Information"
    }
  },
  "AllowedHosts": "*",
  "Server": {
    "Port": 5000
  },
  "CredentialCapture": {
    "LogsDirectory": ".",
    "CredentialsFilename": "captured_credentials.json"
  }
}
